{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "comicvine-images",
      "urls": [
        "https://comicvine.gamespot.com/*"
      ],
      "cacheConfig": {
        "maxSize": 1000,
        "maxAge": "300d",
        "strategy": "performance"
      }
    }
  ]
}